{
  "root_id": {
    "width": "100%",
    "height": "100%",
    "backgroundColor": "transparent"
  },
  "video_id": {
    "extends":[
      "relative_default_layout"
    ],
    "visibility": "${ads[0].materials[0].creative_type} == 8 ? 'Visibility.Visible' : 'Visibility.None'"
  },
  "image_root_id": {
    "width": "100%",
    "height": "100%",
    "visibility": "${ads[0].materials[0].creative_type} == 3 ? 'Visibility.Visible' : 'Visibility.None'"
  },
  "blur_image_id": {
    "extends":[
      "relative_default_layout"
    ],
    "blur": 10,
    "contentMode":"ImageFit.Fill"
  },
  "image_id": {
   "extends":[
      "relative_default_layout"
    ],
     "contentMode":"ImageFit.Contain"
  },
  "blur_image_bg_id": {
   "extends":[
      "relative_default_layout"
    ],
    "blur": 10,
    "contentMode":"ImageFit.Fill"
  },
  "lottie_card_id": {
    "extends":[
      "relative_default_layout"
    ]
  },
  "card_icon_id": {
    "extends":[
      "relative_default_layout"
    ],
    "borderRadius": 15
  },
  "extend": {
      "relative_default_layout": {
        "alignRules": {
          "left": {
          "anchor": "__container__",
          "align": "HorizontalAlign.Start"
          },
        "top": {
          "anchor": "__container__",
          "align": "VerticalAlign.Top"
          }
        },
        "width":"100%",
        "height":"100%"
      }
      
  }
  
}
