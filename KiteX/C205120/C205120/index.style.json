{
  "root_id": {
    "width": "100%",
    "height": "100%",
    "backgroundColor": "transparent"
  },
  "widget_id": {
    "extends":[
      "relative_default_layout"
    ]
  },
  "extend": {
    "background_style": {
      "backgroundColor": "#00000066"
    },
    "background_border_style": {
      "backgroundColor": "#00000066",
      "borderWidth": 1,
      "borderColor": "#ffffff66"
    },
    "relative_default_layout": {
      "alignRules": {
        "left": {
        "anchor": "__container__",
        "align": "HorizontalAlign.Start"
       },
      "top": {
        "anchor": "__container__",
        "align": "VerticalAlign.Top"
       }
      },
      "width":"100%",
      "height":"100%"
    }
  }
  
}
